<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>title</key>
        <string>TubeForge</string>
        <key>items</key>
        <array>

            <!-- Group: Version Settings -->
            <dict>
                <key>cell</key>
                <string>PSGroupCell</string>
                <key>label</key>
                <string>Version Settings</string>
            </dict>

            <!-- Text Field for Custom Version -->
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>defaults</key>
			<string>com.ahmadjerjawi.tubeforgepref</string>
			<key>key</key>
			<string>customVersion</string>
			<key>label</key>
			<string>YouTube Version</string>
			<key>DefaultValue</key>
            <string>18.18</string>
			<key>PostNotification</key>
			<string>com.ahmadjerjawi.tubeforgepref/settingschanged</string>
		</dict>
            <!-- Open Latest Version URL -->
		<dict>
			<key>action</key> <string>version</string>
			<key>cell</key> <string>PSButtonCell</string>
			<key>label</key> <string>Check Out the latest version for youtube</string>
		</dict>

            <!-- Group: Main Page Settings -->
            <dict>
                <key>cell</key>
                <string>PSGroupCell</string>
                <key>label</key>
                <string>Main Page Settings</string>
            </dict>

            <!-- Toggle Home Page -->
            <dict>
                <key>cell</key>
                <string>PSSwitchCell</string>
                <key>label</key>
                <string>Home Page</string>
                <key>key</key>
                <string>wantsHome</string>
                <key>DefaultValue</key>
                <true/>
                <key>defaults</key>
                <string>com.ahmadjerjawi.tubeforgepref</string>
            </dict>

            <!-- Toggle Search Page -->
            <dict>
                <key>cell</key>
                <string>PSSwitchCell</string>
                <key>label</key>
                <string>Search Page</string>
                <key>key</key>
                <string>wantsSearch</string>
                <key>DefaultValue</key>
                <true/>
                <key>defaults</key>
                <string>com.ahmadjerjawi.tubeforgepref</string>
            </dict>

            <!-- Group: Navigation Settings -->
            <dict>
                <key>cell</key>
                <string>PSGroupCell</string>
                <key>label</key>
                <string>Navigation Settings</string>
            </dict>

            <!-- Toggle Side Menu -->
            <dict>
                <key>cell</key>
                <string>PSSwitchCell</string>
                <key>label</key>
                <string>Side Menu</string>
                <key>key</key>
                <string>wantsMenu</string>
                <key>DefaultValue</key>
                <true/>
                <key>defaults</key>
                <string>com.ahmadjerjawi.tubeforgepref</string>
            </dict>

            <!-- Group: Redirect Settings -->
            <dict>
                <key>cell</key>
                <string>PSGroupCell</string>
                <key>label</key>
                <string>Redirect Settings</string>
            </dict>

            <!-- Redirect Home Page -->
            <dict>
                <key>cell</key>
                <string>PSSwitchCell</string>
                <key>label</key>
                <string>Redirect Home Page</string>
                <key>key</key>
                <string>wantsRedirectHome</string>
                <key>DefaultValue</key>
                <false/>
                <key>defaults</key>
                <string>com.ahmadjerjawi.tubeforgepref</string>
            </dict>

            <!-- Redirect Search Page -->
            <dict>
                <key>cell</key>
                <string>PSSwitchCell</string>
                <key>label</key>
                <string>Redirect Search Page</string>
                <key>key</key>
                <string>wantsRedirectSearch</string>
                <key>DefaultValue</key>
                <false/>
                <key>defaults</key>
                <string>com.ahmadjerjawi.tubeforgepref</string>
            </dict>

            <!-- Group: About Section -->
            <dict>
                <key>cell</key>
                <string>PSGroupCell</string>
                <key>label</key>
                <string>About</string>
                <key>footerText</key>
                <string>TubeForge is a tweak that enhances YouTube with smart automation, fixed search options, and improved channel pages.</string>
            </dict>

        </array>
    </dict>
</plist>
